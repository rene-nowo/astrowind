---
import Layout from '~/layouts/PageLayout.astro';
import HeroText from '~/components/widgets/HeroText.astro';
import Prices from '~/components/widgets/Pricing.astro';
import FAQs from '~/components/widgets/FAQs.astro';
import Steps from '~/components/widgets/Steps.astro';
import Features3 from '~/components/widgets/Features3.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';

const metadata = {
  title: 'Pricing',
};
---

<Layout metadata={metadata}>
  <!-- HeroText Widget ******************* -->

  <HeroText
    tagline="Pricing"
    title="QA365 is free to use for partners."
    subtitle="Our App did not officially launch yet. Become a partner and use it now and with no costs."
  />

  <!-- Pricing Widget ******************* -->

  <CallToAction
    title="Get to know the partners program."
    subtitle="Become a partner and use our app. We will handle your onboarding and ticket testing for free."
    actions={[
      {
        variant: 'primary',
        text: 'Get started now',
        href: '/partners',
      },
    ]}
  />
</Layout>
